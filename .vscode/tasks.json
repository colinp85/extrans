{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "check",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo check"
		},
		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "rust: cargo test"
		},
		{
			"type": "cargo",
			"command": "run",
			"args": ["--example", "example"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run example"
		}
	]
}